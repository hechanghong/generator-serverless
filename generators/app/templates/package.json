{
  "name": "<%= projectName %>",
  "version": "<%= projectVersion %>",
  "description": "<%= projectDescription %>",
  "private": true,
  "scripts": {
    "ci": "bash bin/ci.sh",
    "test": "mocha --timeout 30000 --colors --recursive test/**/*.test.js",
    "test-nyan": "mocha --reporter nyan --timeout 30000 --colors --recursive test/**/*.test.js",
    "deploy-dev": "bash bin/deploy.sh dev",
    "deploy-prod": "bash bin/deploy.sh production",
    "security": "nsp check",
    "lint": "eslint src test"
  },
  "keywords": [
    "api",
    "lambda",
    "node",
    "js"
  ],
  "author": "<%= authorName %> <<%= authorEmail %>>",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.4.7",
    "debug": "2.3.3",
    "http-status": "^0.2.3",
    "joi": "10.0.5",
    "winston": "2.3.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "3.12.0",
    "eslint-config-exaprint": "latest",
    "ink-docstrap": "1.3.0",
    "jenkins-mocha": "3.0.4",
    "jsdoc": "3.4.3",
    "nsp": "2.6.2",
    "plato": "1.7.0",
    "serverless-offline": "3.5.7"
  }
}
